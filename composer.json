{
    "name": "jwalbeli/crudapi",
    "description": "simple package for test crud api",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "dani nugrahadi pradana",
            "email": "dani.nugrahadi@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Jwalbeli\\Crudapi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Jwalbeli\\Crudapi\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "9.5.x-dev",
        "orchestra/testbench": "^3.8|^4.0|^5.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jwalbeli\\Crudapi\\DemoPackageServiceProvider"
            ]
        }
    },
    "require": {
        "guzzlehttp/guzzle": "7.4.x-dev"
    }
}
